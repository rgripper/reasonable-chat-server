{
  "name": "reasonable-chat-server",
  "main": "lib/js/src/Index.bs.js",
  "version": "0.1.0",
  "scripts": {
    "dist": "rollup -c",
    "build": "bsb -make-world",
    "postinstall": "bsb -clean-world && bsb -make-world",
