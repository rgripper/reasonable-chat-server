    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "^4.0.18",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9"
  },
  "dependencies": {
    "@glennsl/bs-json": "^3.0.0",
    "@types/socket.io": "^2.1.2",
    "socket.io": "^2.2.0"
  },
  "engines": {
    "node": "10.6.0"
  }
}
